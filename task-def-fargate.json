{
    "containerDefinitions": [
            {
                "name": "task-planner",
                "image": "zaibfridi/book-inventory:v_%BUILD_NUMBER%",
                "cpu": 0,
                "memory": 1024,
                "memoryReservation": 512,
                "portMappings": [
                    {
                        "containerPort": 5000,
                        "hostPort": 5000,
                        "protocol": "tcp"
                    }
                ],
                "essential": true,
                "environment": [],
                "mountPoints": [],
                "volumesFrom": [],
                "logConfiguration": {
                    "logDriver": "awslogs",
                    "options": {
                        "awslogs-group": "/ecs/task-fargate-a",
                        "awslogs-region": "us-east-1",
                        "awslogs-stream-prefix": "ecs"
                    }
                }
            }
        ],
        "family": "task-fargate-a",
        "taskRoleArn": "arn:aws:iam::095861561296:role/ecs-task-role",
        "executionRoleArn": "arn:aws:iam::095861561296:role/ecs-task-role",
        "networkMode": "awsvpc",
        "volumes": [],
        "placementConstraints": [],
        "requiresCompatibilities": [
            "FARGATE"
        ],
        "cpu": "1024",
        "memory": "2048"
    
}
